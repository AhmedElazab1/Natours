{
  "name": "natours",
  "version": "1.0.0",
  "description": "learning node, express and mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "set NODE_ENV=production&&nodemon server.js",
    "start:load": "set NODE_ENV=LOADER&&nodemon server.js",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled",
    "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "author": "Ahmed Elazab",
  "license": "ISC",
